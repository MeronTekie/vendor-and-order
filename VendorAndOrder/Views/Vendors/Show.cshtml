<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Jobs Here</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
    integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
  <link rel="stylesheet" type="text/css" href="~/css/styles.css" />
</head>
<body>
  @using VendorAndOrder.Models;

    <h1>List Of Order For the Vendor:</h1>
    <ul>
    @foreach (Order order in @Model["order"])
    {
      <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Title</a></li>
    }
    </ul>

    <p><a href='/vendors/@Model["vendor"].Id/orders/new'>Add a new order</a></p>
    <p><a href='/vendors'>Return To Vendor</a></p>
    <p><a href='/'>Home</a></p> 
</body>
</html>